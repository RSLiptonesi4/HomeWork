using System;

namespace FirstProject {
    internal class Program {
        static void Main(string[] args) 
        {
string first = "Jason";
string second = "Stepler";
Console.WriteLine("Regular state");
Console.WriteLine("Name:" + first);
Console.WriteLine("Surname:" + second);
string conversely = first;
first = second;
second = conversely;
Console.WriteLine("Reserve state");
Console.WriteLine("Surname:" + first);
Console.WriteLine("Name:" + second);

string firstCup = "Tea";
string secondCup = "Jucy";
Console.WriteLine("First state");
Console.WriteLine("First cup:" + firstCup);
Console.WriteLine("Second cup:" + secondCup);
string round = firstCup;
firstCup = secondCup;
secondCup = round;
Console.WriteLine("Second state");
Console.WriteLine("Second cup:" + firstCup);
Console.WriteLine("First cup:" + secondCup);


        }

    }
}

