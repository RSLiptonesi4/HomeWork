using System;

namespace FirstProject {
    internal class Program {
        static void Main(string[] args) 
        {
            int pictures = 52;
            int picturesInRow = 3;
            int completeRows = pictures / picturesInRow;
            int leftoverPictures = pictures % picturesInRow;
            Console.WriteLine("Всего полных рядов: " + completeRows);
            Console.WriteLine("Осталось всего картин: " + leftoverPictures);

        }

    }
}

