using System;

namespace FirstProject {
    internal class Program {
        static void Main(string[] args) 
        {
string first = "Jason";
string second = "Stepler";
Console.WriteLine("Обычное состояние");
Console.WriteLine("Имя:" + first);
Console.WriteLine("Фамилия:" + second);
string conversely = first;
first = second;
second = conversely;
Console.WriteLine("Наоборот состояние");
Console.WriteLine("Фамилия:" + first);
Console.WriteLine("Имя:" + second);
        }

    }
}

